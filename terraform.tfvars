# Terraform Variables

# AWS Configuration
region = "us-east-1"

# Environment
environment = "dev"

# Project
project = "github-schema-amazon-q"

# S3 Buckets
source_bucket = "bucket-1"
output_bucket = "bucket-2"

# SSO Account
sso_account = "123456789012"

# Amazon Q Configuration
amazon_q_index_name = "amazon-q-index-name"
amazon_q_instance_name = "amazon-q-instance-name"

# Lambda Configuration
lambda_timeout = 300
lambda_memory = 512

# SQS Queue Settings
queue_name = "sqs-queue-name"
